[application]

config/name="Snail Quest"
config/description="A small snail on an adventure to save his homeland! "
run/main_scene="res://assets/gui/gui_screen_title.tscn"
config/use_hidden_project_data_directory=false
config/use_custom_user_dir=true
boot_splash/image="res://assets/texture/gui/gui_splash.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Clock="*res://source/Clock.gd"
SaveData="*res://source/SaveData.gd"
GuiDebug="*res://assets/gui/gui_popup_debug.tscn"
AudioPlayer="res://assets/object/audio_player.tscn"
WorldAudioPlayer="res://assets/object/world_audio_player.tscn"
Library="*res://addons/everything_library/src/autoload.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

scene_naming=1
reimport_missing_imported_files=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogic/plugin.cfg", "res://addons/epic-anchors/plugin.cfg", "res://addons/everything_library/plugin.cfg", "res://addons/gamelevels_blueprint/plugin.cfg", "res://addons/resources_speadsheet_view/plugin.cfg" )

