[application]

config/name="Snail Quest"
config/description="A small snail on an adventure to save his homeland! "
run/main_scene="res://assets/gui/gui_title.tscn"
config/use_hidden_project_data_directory=false
config/use_custom_user_dir=true
boot_splash/image="res://splash.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

GuiDebug="*res://assets/gui/gui_debug.tscn"
WorldClock="*res://source/WorldClock.gd"
DataSave="*res://source/WorldSaveData.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/width=1920
window/size/height=1080
window/dpi/allow_hidpi=true
window/per_pixel_transparency/allowed=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

reimport_missing_imported_files=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogue_manager/plugin.cfg", "res://addons/epic-anchors/plugin.cfg", "res://addons/gamelevels_blueprint/plugin.cfg", "res://addons/project_map/plugin.cfg" )

